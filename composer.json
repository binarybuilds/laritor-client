{
    "name": "binarybuilds/laritor-client",
    "description": "Laritor is a application performance monitoring and observability tool for applications built using laravel php framework",
    "keywords": [
        "laritor",
        "performance monitoring for laravel",
        "observability for laravel"
    ],
    "homepage": "https://github.com/binarybuilds/laritor-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Laritor",
            "email": "support@laritor.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3|^8.4",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^8.0.0|^9.0.0|^10.0.0|^11.0.0|^12.0.0",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "BinaryBuilds\\LaritorClient\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BinaryBuilds\\LaritorClient\\LaritorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/",
            "BinaryBuilds\\LaritorClient\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "test": [
            "@clear",
            "@php vendor/bin/phpunit"
        ]
    }
}