{
    "name": "laritor/laravel-client",
    "description": "Laritor is a laravel health and application performance monitoring tool",
	"keywords": [
        "laritor",
        "laravel",
        "laravel monitoring tool",
        "performance monitoring"
    ],
    "homepage": "https://github.com/:vendor_slug/:package_slug",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": "author@domain.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^7.3|^7.4|^8.0|^8.1|^8.2|^8.3",
        "laravel/framework": ">=7.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Laritor\\LaravelClient\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laritor\\LaravelClient\\LaritorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
