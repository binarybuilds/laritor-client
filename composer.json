{
    "name": "laritor/laravel-client",
    "description": "A hello world composer private package.",
	"keywords": [
        ":vendor_name",
        "laravel",
        ":package_slug"
    ],
    "homepage": "https://github.com/:vendor_slug/:package_slug",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": "author@domain.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^7.3|^7.5|^8.0|^8.1|^8.2|^8.3"
    },
    "autoload": {
        "psr-4": {
            "Laritor\\LaravelClient\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laritor\\LaravelClient\\LaritorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
